{
  "version": 2,
  "env": {
    "YANDEX_AUTH_TOKEN": "@yandex_auth_token"
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "PATCH"
          ]
      }
  ]
}